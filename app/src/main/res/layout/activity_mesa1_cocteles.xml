<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".mesa1.Mesa1Cocteles">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:autoSizeTextType="uniform"
        android:background="#C4E5FF"
        android:gravity="center"
        android:text="@string/cocteles"
        android:textAlignment="center"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/ini1"
        style="@style/Widget.AppCompat.Button.Colored"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="none"
        android:onClick="aMenu1"
        android:text="@string/inicio"
        android:textSize="34sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView6" />

    <Button
        android:id="@+id/tot1"
        style="@style/Widget.Material3.Button"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:autoSizeTextType="none"
        android:backgroundTint="#A9FFAD"
        android:onClick="aTotal1"
        android:text="@string/total"
        android:textColor="#000000"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <ListView
        android:id="@+id/listviewcocteles1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toTopOf="@+id/textView7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/textView4"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:background="#C4E5FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView7"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="SpeakableTextPresentCheck,SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="20dp"
        android:layout_height="0dp"
        android:background="#C4E5FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="0dp"
        android:layout_height="20dp"
        android:layout_marginBottom="100dp"
        android:background="#C4E5FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="250dp"
        android:layout_height="20dp"
        android:layout_marginBottom="100dp"
        android:background="#C4E5FF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/tial1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/dai1"
        app:layout_constraintEnd_toStartOf="@+id/rfon1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <Button
        android:id="@+id/rfon1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/tial1"
        app:layout_constraintEnd_toStartOf="@+id/ardin1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/tial1"
        app:layout_constraintTop_toTopOf="@+id/tial1" />

    <Button
        android:id="@+id/ardin1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/rfon1"
        app:layout_constraintEnd_toStartOf="@+id/gfizz1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/rfon1"
        app:layout_constraintTop_toTopOf="@+id/rfon1" />

    <Button
        android:id="@+id/gfizz1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/ardin1"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ardin1"
        app:layout_constraintTop_toTopOf="@+id/ardin1" />

    <Button
        android:id="@+id/dai1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/pisour1"
        app:layout_constraintEnd_toStartOf="@+id/moj1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/tial1" />

    <Button
        android:id="@+id/moj1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/dai1"
        app:layout_constraintEnd_toStartOf="@+id/whsour1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/dai1"
        app:layout_constraintTop_toTopOf="@+id/dai1"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/whsour1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/moj1"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/moj1"
        app:layout_constraintTop_toTopOf="@+id/moj1" />

    <Button
        android:id="@+id/sincol1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/caipi1"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/caipi1"
        app:layout_constraintTop_toTopOf="@+id/caipi1" />

    <Button
        android:id="@+id/pisour1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/marga1"
        app:layout_constraintEnd_toStartOf="@+id/moscowmule1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/dai1" />

    <Button
        android:id="@+id/moscowmule1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/pisour1"
        app:layout_constraintEnd_toStartOf="@+id/oldfas1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/pisour1"
        app:layout_constraintTop_toTopOf="@+id/pisour1" />

    <Button
        android:id="@+id/oldfas1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/moscowmule1"
        app:layout_constraintEnd_toStartOf="@+id/textView4"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/moscowmule1"
        app:layout_constraintTop_toTopOf="@+id/moscowmule1" />

    <Button
        android:id="@+id/caipi1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toBottomOf="@+id/marga1"
        app:layout_constraintEnd_toStartOf="@+id/sincol1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/marga1"
        app:layout_constraintTop_toTopOf="@+id/marga1" />

    <Button
        android:id="@+id/marga1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text=""
        android:textSize="28sp"
        app:layout_constraintBottom_toTopOf="@+id/textView6"
        app:layout_constraintEnd_toStartOf="@+id/caipi1"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/textView5"
        app:layout_constraintTop_toBottomOf="@+id/pisour1" />


</androidx.constraintlayout.widget.ConstraintLayout>